<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#4CAF50">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="346dp"
        android:layout_height="123dp"
        android:layout_marginBottom="56dp"

        android:gravity="center"
        android:text="Thank You"
        android:textSize="30dp"
        android:textColor="#0E0E0E"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/main"
        android:layout_width="395dp"
        android:layout_height="190dp"
        android:text=""
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="4dp" />

    <TextView
        android:id="@+id/n4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" Do you have supportive classmates?"
        android:textStyle="bold"
        android:textSize="20dp"
        android:textColor="#151515"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp" />

    <RadioButton
        android:id="@+id/radioButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Yes, extremely supportive"
        android:textColor="#0C0C0C"
        tools:layout_editor_absoluteX="17dp"
        tools:layout_editor_absoluteY="51dp" />

    <RadioButton
        android:id="@+id/radioButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#0C0C0C"
        android:text="They are neither supportive nor unsupportive"
        tools:layout_editor_absoluteX="17dp"
        tools:layout_editor_absoluteY="96dp" />

    <RadioButton
        android:id="@+id/radioButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#0C0C0C"
        android:text="No, extremely unsupportive"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="149dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="388dp"
        android:layout_height="280dp"

        tools:layout_editor_absoluteX="3dp"
        tools:layout_editor_absoluteY="203dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="How much time do you spend every day on homework?"
        android:textSize="17dp"
        android:textStyle="bold"
        android:textColor="#0C0C0C"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="203dp" />

    <RadioButton
        android:id="@+id/radioButton4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="<2 Hours"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="265dp" />

    <RadioButton
        android:id="@+id/radioButton5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2-3 Hours"
        tools:layout_editor_absoluteX="27dp"
        tools:layout_editor_absoluteY="315dp" />

    <RadioButton
        android:id="@+id/radioButton6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="3-4 Hours"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="364dp" />

    <RadioButton
        android:id="@+id/radioButton7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=">5 Hours"
        tools:layout_editor_absoluteX="27dp"
        tools:layout_editor_absoluteY="411dp" />

</androidx.constraintlayout.widget.ConstraintLayout>